<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="mageplaza" translate="label" sortOrder="400">
            <label>mageplaza</label>
        </tab>
        <section id="seo" translate="label" type="text" sortOrder="550" showInDefault="1" showInWebsite="1" showInStore="1">
            <class>separator-top</class>
            <label>Seo</label>
            <tab>mageplaza</tab>
            <resource>Mageplaza_Seo::seo</resource>
            <group id="general" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1">
                <label>General</label>
                <field id="is_enabled" translate="label comment" type="select" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Enable</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
            </group>
        </section>
    </system>
</config>
